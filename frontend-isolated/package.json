{
  "name": "frontend-isolated",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "powershell ./scripts/start-dev.ps1",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "serve dist -s -n -L -p $PORT",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:dev": "powershell ./e2e/scripts/run-dev-tests.ps1",
    "test:e2e:dev:debug": "powershell ./e2e/scripts/start-dev-services.ps1 && playwright test dev-mode.spec.ts --debug",
    "test:e2e:prod": "SKIP_DEV_SERVERS=true playwright test",
    "test:dev:api": "powershell ./e2e/scripts/test-api-dev.ps1",
    "test:dev:jwt": "powershell ./e2e/scripts/test-jwt-dev.ps1",
    "test:dev:ulc": "powershell ./e2e/scripts/test-ulc-dev.ps1"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "playwright": "^1.50.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.14.4",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/ui": "^3.0.5",
    "autoprefixer": "^10.4.15",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": "20.x"
  }
}
